program HelloWorld;

var
    newint : integer;
    str: string;

function print (ss : string) : integer;
var
Writeln(ss);

begin
    newint := 10;
    scan(newint);
    scan(str);
    
    if newint >= 0 then
        print(str)
    else
        begin
            newint := newint + 5;
            Writeln(newint);
        end;


end.
